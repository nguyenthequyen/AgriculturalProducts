    <script src="https://www.google.com/recaptcha/api.js?render=6LcByJcUAAAAAPYDK4Ai_E4cCZIYifpUUEXcxaOO"></script>
    <script>
        grecaptcha.ready(function () {
            grecaptcha.execute('6LcByJcUAAAAAPYDK4Ai_E4cCZIYifpUUEXcxaOO', { action: 'homepage' }).then(function (token) {
                var data = {
                    token: token
                }
                $(renderAPI.postAPI(DOMAIN + "api/HomeAdmin/secure-website", true, 'post', JSON.stringify(data),
                    function (result) {
                        if (result.data === "error") {
                            window.location.href = DOMAIN + "Home/Securety";
                        }
                    }, function (xhr, status) {
                        console.log(xhr);
                    }));
            });
        });
    </script>
